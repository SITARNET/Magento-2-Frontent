<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template"  name="test" template="Magento_Theme::test.phtml">
                <arguments>
                    <argument name="css_class" xsi:type="string">items order-links</argument>
                </arguments>
            <block class="Magento\Framework\View\Element\Html\Link" name="test_link" >
                <arguments>
                    <argument name="title" xsi:type="string">link title</argument>
                    <argument name="href" xsi:type="string">/contact</argument>
                    <argument name="label" xsi:type="string">Contact</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Html\Link" name="test_link1" >
                <arguments>
                    <argument name="title" xsi:type="string">link title1</argument>
                    <argument name="href" xsi:type="string">/contact1</argument>
                    <argument name="label" xsi:type="string">Contact1</argument>
                </arguments>
            </block>
            </block>
        </referenceContainer>
    </body>
</page>
